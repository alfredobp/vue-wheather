nsole.error(error);\n        this.error = true;\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+FA;AACA,kC,CACA;;AACA;EACAA;IACAC,IADA;IAEAC,UAFA;IAGAC;EAHA,CADA;EAMAC,WANA;;EAOAC;IACA;MACAC,YADA;MAEAC,OAFA;MAGAC,QAHA;MAIAC,gBAJA;MAKAJ,QALA;MAMAK,YANA;MAOAC,yDAPA;MAQAC,aACA,0FATA;MAUAC,QAVA;MAWAC,oCAXA;MAYAC;IAZA;EAcA,CAtBA;;EAwBAC;IACA;MACA;MACAC;IACA,CAHA,CAGA;MACAC;IACA;EACA,CA/BA;;EAgCAC;IACAC;MACA;MACA;MACA;MACAF;MACA;IACA,CAPA;;IAQAG;MACA;;MACA;MACA;QACAC,aADA;QAEAX,oDAFA;QAGAY;UACAhB,yBADA;UAEAC,8BAFA;UAGAgB,cAHA;UAIAC;QAJA,CAHA;QASAC;UACA,sEADA;UAEA;QAFA;MATA;MAeAC,MACAC,OADA,CACAC,OADA,EAEAC,IAFA,CAEA;QACAZ;QACAa;QACAA;MACA,CANA,EAOAC,KAPA,CAOA;QACAd;QACA;MACA,CAVA;IAWA;;EArCA;AAhCA","names":["components","LMap","LTileLayer","LMarker","name","data","isLoad","lat","long","api","error","url","attribution","zoom","center","markerLatLng","mounted","navigator","console","methods","showPosition","getInfo","method","params","format","u","headers","axios","request","options","then","vm","catch"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <div v-if=\"isLoad\">\n      <h3>Cargando...</h3>\n    </div>\n    <div else>\n      <h1>Información municipal</h1>\n      <l-map style=\"height: 300px\" :zoom=\"zoom\" :center=\"center\">\n        <l-tile-layer :url=\"url\" :attribution=\"attribution\"></l-tile-layer>\n        <l-marker :lat-lng=\"markerLatLng\"></l-marker>\n      </l-map>\n      <sui-container>\n        <div class=\"\" v-if=\"!error\">\n          <div class=\"col-md-6\" style=\"width: 50%; margin: 0 auto\">\n            <h1 is=\"sui-segment\" inverted>\n              Previsión Meterológica para: {{ data.location.city }}\n            </h1>\n            <sui-divider horizontal>Datos viento</sui-divider>\n            <div class=\"ui vertically divided grid\">\n              <div class=\"three column row\">\n                <div class=\"column\">\n                  <p>\n                    Sensación térmica:\n                    {{\n                      (((data.current_observation.wind.chill - 32) * 5) / 9).toFixed(2)\n                    }}\n                    ªC\n                  </p>\n                </div>\n                <div class=\"column\">\n                  <p>\n                    <img src=\"./assets/direccion-del-viento.png\" width=\"30px\" />\n                    {{ data.current_observation.wind.direction }}\n                  </p>\n                </div>\n                <div class=\"column\">\n                  <p>\n                    <img src=\"./assets/presion.png\" width=\"30px\" />\n\n                    {{ data.current_observation.wind.speed }}\n                  </p>\n                </div>\n              </div>\n            </div>\n            <sui-divider horizontal>Datos Atmósfericos</sui-divider>\n <div class=\"ui vertically divided grid\">\n              <div class=\"three column row\">\n                <div class=\"column\">\n                  <p>\n            <img src=\"./assets/presion.png\" width=\"30px\" />\n            {{ data.current_observation.atmosphere.pressure }}\n                  </p>\n                </div>\n                  <div class=\"column\">\n                  <p>\n            <img src=\"./assets/visibilidad.png\" width=\"30px\" />\n            {{ data.current_observation.atmosphere.visibility }}\n                  </p>\n                </div>\n                  <div class=\"column\">\n                  <p>\n            <img src=\"./assets/humedad.png\" width=\"30px\" />\n            {{ data.current_observation.atmosphere.humidity }}\n                  </p>\n                </div>\n              </div>\n </div>\n            {{ data.current_observation.atmosphere }}\n\n\n            <fieldset>\n              <legend>Datos actuales</legend>\n              <img src=\"./assets/thermometer.png\" width=\"30px\" />\n              Tª:\n              {{\n                (((data.current_observation.condition.temperature - 32) * 5) / 9).toFixed(\n                  2\n                )\n              }}\n              ªC\n              <br />\n              {{ data.current_observation }}\n            </fieldset>\n            <aside>Datos:</aside>\n            <div id=\"demo\"></div>\n          </div>\n        </div>\n\n        <div v-else>Se ha producido un eror</div>\n      </sui-container>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { LMap, LTileLayer, LMarker } from \"vue2-leaflet\";\nimport \"leaflet/dist/leaflet.css\";\n// import HelloWorld from \"./components/HelloWorld.vue\";\nexport default {\n  components: {\n    LMap,\n    LTileLayer,\n    LMarker,\n  },\n  name: \"App\",\n  data() {\n    return {\n      isLoad: true,\n      lat: \"\",\n      long: \"\",\n      api: \"google.es\",\n      data: \"\",\n      error: false,\n      url: \"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\",\n      attribution:\n        '&copy; <a target=\"_blank\" href=\"http://osm.org/copyright\">OpenStreetMap</a> contributors',\n      zoom: 15,\n      center: [36.7738869, -6.3545177, 15],\n      markerLatLng: [36.7738869, -6.3545177, 15],\n    };\n  },\n\n  mounted() {\n    try {\n      this.isLoad = true;\n      navigator.geolocation.getCurrentPosition(this.showPosition);\n    } catch (error) {\n      console.error(error);\n    }\n  },\n  methods: {\n    showPosition(position) {\n      this.lat = position.coords.latitude;\n      this.longitude = position.coords.longitude;\n      this.center = [this.lat, this.longitude];\n      console.log(this.center);\n      this.getInfo(position.coords.latitude, position.coords.longitude);\n    },\n    getInfo(a, b) {\n      const axios = require(\"axios\");\n      var vm = this;\n      const options = {\n        method: \"GET\",\n        url: \"https://yahoo-weather5.p.rapidapi.com/weather\",\n        params: {\n          lat: a ? a : \"36.7738869\",\n          long: b ? b : \"-6.3545177,15z\",\n          format: \"json\",\n          u: \"f\",\n        },\n        headers: {\n          \"X-RapidAPI-Key\": \"99833a4266mshe439dc5ecc4747ap155b42jsnab34657115fe\",\n          \"X-RapidAPI-Host\": \"yahoo-weather5.p.rapidapi.com\",\n        },\n      };\n\n      axios\n        .request(options)\n        .then(function (response) {\n          console.log(response.data);\n          vm.data = response.data;\n          vm.isLoad = false;\n        })\n        .catch(function (error) {\n          console.error(error);\n          this.error = true;\n        });\n    },\n  },\n};\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}